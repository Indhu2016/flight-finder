{
  "name": "smart-travel-optimizer",
  "version": "1.0.0",
  "description": "Agentic AI Travel Route Optimizer - Find optimal travel routes using autonomous AI agents",
  "main": "main.py",
  "type": "module",
  "scripts": {
    "dev": "streamlit run streamlit_app.py",
    "start": "python main.py",
    "install": "pip install -r requirements.txt",
    "test": "pytest",
    "format": "black src/ main.py streamlit_app.py tests/",
    "lint": "ruff check src/ main.py streamlit_app.py tests/",
    "security": "bandit -r src/ main.py streamlit_app.py",
    "sonar": "sonar-scanner"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Indhu2016/flight-finder.git"
  },
  "keywords": [
    "agentic-ai",
    "autonomous-agent",
    "travel-optimization",
    "route-planning",
    "python",
    "streamlit",
    "ai-agent",
    "multi-criteria-optimization",
    "travel-ai"
  ],
  "author": "Smart Travel Team <team@smarttravel.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Indhu2016/flight-finder/issues"
  },
  "homepage": "https://github.com/Indhu2016/flight-finder#readme",
  "devDependencies": {
    "sonarqube-scanner": "^3.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0",
    "python": ">=3.8"
  }

}
